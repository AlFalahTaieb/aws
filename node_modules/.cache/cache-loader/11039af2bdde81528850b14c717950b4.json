nedId\": \"notes_dr\",\n  \"id\": \"notes_dr\",\n  \"isDocsHomePage\": false,\n  \"title\": \"Disaster Recovery\",\n  \"description\": \"export const Quote = ({ children, color }) => (\",\n  \"source\": \"@site/docs/notes_dr.mdx\",\n  \"slug\": \"/notes_dr\",\n  \"permalink\": \"/FAQ/docs/notes_dr\",\n  \"version\": \"current\",\n  \"sidebar\": \"someSidebar\",\n  \"previous\": {\n    \"title\": \"Other Services\",\n    \"permalink\": \"/FAQ/docs/notes_os\"\n  },\n  \"next\": {\n    \"title\": \"HPC\",\n    \"permalink\": \"/FAQ/docs/notes_hpc\"\n  }\n};\n  /* @jsx mdx */\nimport Tabs from '@theme/Tabs';\nimport TabItem from '@theme/TabItem';\nexport const rightToc = [{\n  value: 'I. Introduction',\n  id: 'i-introduction',\n  children: []\n}, {\n  value: 'II. DR Strategies',\n  id: 'ii-dr-strategies',\n  children: []\n}, {\n  value: 'III. DR Tips',\n  id: 'iii-dr-tips',\n  children: []\n}, {\n  value: 'IV. Database Migration Service (DMS)',\n  id: 'iv-database-migration-service-dms',\n  children: []\n}, {\n  value: 'V. On-premise strategy with AWS',\n  id: 'v-on-premise-strategy-with-aws',\n  children: []\n}, {\n  value: 'VI. Data transfer scenarios',\n  id: 'vi-data-transfer-scenarios',\n  children: []\n}];\nexport const Quote = ({\n  children,\n  color\n}) => <span style={{\n  backgroundColor: \"#5a5a5a\",\n  borderRadius: \"8px\",\n  color: \"#FF9900\",\n  padding: \"0.21rem\"\n}}>\n\t\t{children}\n\t</span>;\n\nconst layoutProps = {\n  rightToc,\nQuote\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n\n\n    {\n      /* ![exp AWS](../src/media/exp.png) */\n    }\n    <h2 {...{\n      \"id\": \"i-introduction\"\n    }}>{`I. Introduction`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Any event that has a negative impact on a company's business continuity or finances is a disaster.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Disaster Recovery (DR) is about preparing and recovering from it.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Kinds of DR`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`On premise → traditional, very expensive`}</li>\n          <li parentName=\"ol\">{`On premise → AWS cloud hybrid recovery`}</li>\n          <li parentName=\"ol\">{`Full cloud → AWS region A to B`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Recovery Point Objective (RPO) vs Recovery Time Objective (RTO)`}</p>\n      </li>\n    </ol>\n    <p><img alt={\"Disaster Recovery AWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/dr1.png\").default} /></p>\n    <h2 {...{\n      \"id\": \"ii-dr-strategies\"\n    }}>{`II. DR Strategies`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`Backup and restore`}\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`High RPO and RTO → hours`}</li>\n          <li parentName=\"ol\">{`No managing infra`}</li>\n          <li parentName=\"ol\">{`Just restore from most recent point in time`}</li>\n          <li parentName=\"ol\">{`Keep snapshots of EBS volumes, RDS, S3 (or Glacier)`}</li>\n        </ol>\n      </li>\n    </ol>\n    <p><img alt={\"Disaster Recovery AWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/dr2.png\").default} /></p>\n    <ol {...{\n      \"start\": 2\n    }}>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Pilot light`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`RPO and RTO → 10s of minutes`}</li>\n          <li parentName=\"ol\">{`A small flame that’s always on can quickly ignite the entire furnace to heat up a house`}</li>\n          <li parentName=\"ol\"><strong parentName=\"li\">{`Most critical core elements are running`}</strong></li>\n          <li parentName=\"ol\">{`When the time comes, you can rapidly provision a full-scale production app`}</li>\n          <li parentName=\"ol\">{`It is an Active/Passive failover configuration`}</li>\n          <li parentName=\"ol\">{`Restore happens through EBS snapshots and AMIs`}</li>\n          <li parentName=\"ol\">{`Lower RTO/RPO than backup because your app will always running`}</li>\n          <li parentName=\"ol\">{`Faster than backup because critical systems are already up`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Full-working low-capacity (warm) standby`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`RPO and RTO → minutes`}</li>\n          <li parentName=\"ol\"><strong parentName=\"li\">{`Scaled-down version of a fully functional system is always running`}</strong></li>\n          <li parentName=\"ol\">{`Another example of Active/Passive failover configuration`}</li>\n          <li parentName=\"ol\">{`Servers can be left running on minimum and `}<strong parentName=\"li\">{`smallest EC2s`}</strong></li>\n          <li parentName=\"ol\">{`Upon disaster, we can scale it to production load by increasing instances`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Multi-site Active-Active`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Very low RTO (minutes or seconds)`}</li>\n          <li parentName=\"ol\">{`Most expensive`}</li>\n          <li parentName=\"ol\">{`Full production scale is running on one AWS region and on premise`}</li>\n          <li parentName=\"ol\">{`You can just change DNS and be good to go in case of failover`}</li>\n        </ol>\n      </li>\n    </ol>\n    <p><img alt={\"Disaster Recovery AWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/dr3.png\").default} /></p>\n    <h2 {...{\n      \"id\": \"iii-dr-tips\"\n    }}>{`III. DR Tips`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Backup`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`EBS snapshots, RDS automated backups`}</li>\n          <li parentName=\"ol\">{`Regular pushes to S3, S3 IA, Glacier (with lifecycle policy)`}</li>\n          <li parentName=\"ol\">{`From on-prem: Snowball or Storage Gateway`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`HA`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Use Route53 to migrate from one region to another`}</li>\n          <li parentName=\"ol\">{`RDS multi AZ, ElastiCache multi-AZ, EFS`}</li>\n          <li parentName=\"ol\">{`Site to site VPN as a recovery from Direct Connect`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Replication`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`RDS replication (cross-region), AWS Aurora + Global Database`}</li>\n          <li parentName=\"ol\">{`Database replication from on-prem to RDS`}</li>\n          <li parentName=\"ol\">{`Storage Gateway`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Automation`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`CloudFormation, Beanstalk to re-create full env`}</li>\n          <li parentName=\"ol\">{`Recover/Reboot EC2 with CloudWatch alarms`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Chaos engineering `}</p>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"iv-database-migration-service-dms\"\n    }}>{`IV. Database Migration Service (DMS)`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Quickly and securely migrate db to AWS, resilient, self-healing`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`The source db remains available 100% during migration`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Supports both homogenous and heterogenous migrations`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Oracle to Oracle`}</li>\n          <li parentName=\"ol\">{`Oracle to Pgsql`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Continuous data replication using CDC`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`You must create an EC2 instance to perform replication tasks`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`If your source and target databases are different, then you need to use Schema Conversion Tool (SCT)`}</p>\n      </li>\n    </ol>\n    <p><img alt={\"Disaster Recovery AWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/dr4.png\").default} /></p>\n    <h2 {...{\n      \"id\": \"v-on-premise-strategy-with-aws\"\n    }}>{`V. On-premise strategy with AWS`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Ability to download AMI in .iso format. This can then loaded on popular VM softwares including VMWare, HyperV etc`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`You can also migrate existing applications to EC2 and create a DR strategy for on-prem VMs.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`You can also export back from EC2 to on-prem VM.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`AWS Application Discovery Service`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Gather info about your on-prem servers to plan a migration`}</li>\n          <li parentName=\"ol\">{`Server utilization and dependency mapping`}</li>\n          <li parentName=\"ol\">{`Track with AWS migration hub`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`AWS Server Migration Service (SMS) — Incremental replication of on-premise live servers to AWS`}</p>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"vi-data-transfer-scenarios\"\n    }}>{`VI. Data transfer scenarios`}</h2>\n    <p>{`Scenario: Transferring 200TB of data to cloud over 100Mbps connection`}</p>\n    <ol>\n      <li parentName=\"ol\">{`Over the internet (Site-to-site VPN) — immediate to setup but will take 185 days!`}</li>\n      <li parentName=\"ol\">{`Over direct connect (1Gbps) — long setup time (1 month), 18.5 days`}</li>\n      <li parentName=\"ol\">{`Snowball — 1 week end-to-end transfer`}</li>\n    </ol>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}