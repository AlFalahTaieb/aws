VF,CAjHK,CAoIL,SAAQ,CACN,KAAM,wBADA,CAAR,CAEK,yBAFL,CApIK,CAuIL,cACE,UAAI,UAAU,CAAC,IAAf,EAAoB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,KAA1B,CAApB,CAA8D,iBAA9D,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAoB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,UAA1B,CAApB,CAAmE,kBAAnE,CAFF,CAGE,UAAI,UAAU,CAAC,IAAf,EAAoB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,MAA1B,CAApB,CAA+D,sDAA/D,CAHF,CAIE,UAAI,UAAU,CAAC,IAAf,EAAoB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,kBAA1B,CAApB,CAA2E,yBAA3E,CAJF,CAKE,UAAI,UAAU,CAAC,IAAf,EAAoB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,iBAA1B,CAApB,CAA0E,wBAA1E,CALF,CAME,UAAI,UAAU,CAAC,IAAf,EAAoB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,MAA1B,CAApB,CAA+D,wCAA/D,CANF,CAOE,UAAI,UAAU,CAAC,IAAf,EAAoB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,mBAA1B,CAApB,CAA4E,sCAA5E,CAPF,CAQE,UAAI,UAAU,CAAC,IAAf,EAAoB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,kBAA1B,CAApB,CAA2E,wDAA3E,CARF,CASE,UAAI,UAAU,CAAC,IAAf,EAAoB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,sBAA1B,CAApB,CAA+E,iDAA/E,CATF,CAUE,UAAI,UAAU,CAAC,IAAf,EAAoB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,WAA1B,CAApB,CAAoE,gDAApE,CAVF,CAvIK,CAmJL,SAAQ,CACN,KAAM,6BADA,CAAR,CAEK,8BAFL,CAnJK,CAsJL,cACE,UAAI,UAAU,CAAC,IAAf,EAAsB,6EAAtB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAsB,4CAAtB,CAFF,CAGE,UAAI,UAAU,CAAC,IAAf,EAAsB,iFAAtB,CAHF,CAIE,UAAI,UAAU,CAAC,IAAf,EAAsB,+FAAtB,CAJF,CAKE,UAAI,UAAU,CAAC,IAAf,EAAsB,wCAAtB,CALF,CAtJK,CA6JL,SAAQ,CACN,KAAM,+BADA,CAAR,CAEK,8BAFL,CA7JK,CAgKL,cACE,UAAI,UAAU,CAAC,IAAf,EAAsB,6EAAtB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAsB,4CAAtB,CAFF,CAGE,UAAI,UAAU,CAAC,IAAf,EAAsB,iFAAtB,CAHF,CAIE,UAAI,UAAU,CAAC,IAAf,EAAsB,+FAAtB,CAJF,CAKE,UAAI,UAAU,CAAC,IAAf,EAAsB,wCAAtB,CALF,CAhKK,CAuKL,aAAK,4BAAL,CAvKK,CAwKL,aAAG,WAAK,GAAG,CAAE,sBAAV,CAAkC,GAAG,CAAED,OAAO,CAAC,iHAAD,CAAP,CAA2HC,OAAlK,EAAH,CAxKK,CAAP,CA0KD,CAED,CACAJ,UAAU,CAACK,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tid: 'notes_storage_ex',\n\ttitle: 'Storage Extras',\n\tsidebar_label: 'Storage Extras'\n};\nexport const metadata = {\n  \"unversionedId\": \"notes_storage_ex\",\n  \"id\": \"notes_storage_ex\",\n  \"isDocsHomePage\": false,\n  \"title\": \"Storage Extras\",\n  \"description\": \"export const Highlight = ({ children, color }) => (\",\n  \"source\": \"@site/docs/notes_storage_ex.mdx\",\n  \"slug\": \"/notes_storage_ex\",\n  \"permalink\": \"/FAQ/docs/notes_storage_ex\",\n  \"version\": \"current\",\n  \"sidebar_label\": \"Storage Extras\",\n  \"sidebar\": \"someSidebar\",\n  \"previous\": {\n    \"title\": \"S3\",\n    \"permalink\": \"/FAQ/docs/notes_s3\"\n  },\n  \"next\": {\n    \"title\": \"Athena\",\n    \"permalink\": \"/FAQ/docs/notes_athena\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: 'I. Snowball',\n  id: 'i-snowball',\n  children: []\n}, {\n  value: 'II. Storage Gateway',\n  id: 'ii-storage-gateway',\n  children: []\n}, {\n  value: 'III. Storage comparison',\n  id: 'iii-storage-comparison',\n  children: []\n}, {\n  value: 'IV. S3 Transfer Acceleration',\n  id: 'iv-s3-transfer-acceleration',\n  children: []\n}, {\n  value: 'IV. S3 Transfer Acceleration',\n  id: 'iv-s3-transfer-acceleration-1',\n  children: []\n}];\nexport const Highlight = ({\n  children,\n  color\n}) => <span style={{\n  backgroundColor: \"#FF9900\",\n  borderRadius: \"5px\",\n  color: \"#fff\",\n  padding: \"0.1rem\"\n}}>\n\t\t{children}\n\t</span>;\nexport const Quote = ({\n  children,\n  color\n}) => <span style={{\n  backgroundColor: \"#5a5a5a\",\n  borderRadius: \"7px\",\n  color: \"#FF9900\",\n  padding: \"0.3rem\"\n}}>\n\t\t{children}\n\t</span>;\n\nconst layoutProps = {\n  rightToc,\nHighlight,\nQuote\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n\n\n    <h2 {...{\n      \"id\": \"i-snowball\"\n    }}>{`I. Snowball`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Physical data transport solution that helps moving TBs/PBs of data in/out of AWS.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Alt to moving data over network and paying network fee.</Quote>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Secure, tamper-resistance, uses KMS 256-bit encryption.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Tracking using SNS and text messages with e-ink shipping label.</Quote>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Pay per data transfer job.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Used in large data cloud migrations, Datacenter decommission and disaster recovery.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">If it takes more than a week to transfer over network, consider using Snowball.</Quote>\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Use Snowmobile while transferring more than 10PB</Quote>\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Process</Quote>\n1. Order a snowball from AWS\n2. Install snowball client on your servers\n3. Connect snowball to your server and move files using client\n4. Ship back the device using onboard e-ink shipping label\n5. Data will be loaded to S3 once snowball reaches AWS facility\n6. Snowball is then completely wiped\n7. Tracking of every step using SNS, text and console\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Snowball edge is a new service that brings compute power to snowball`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Snowball edge comes with 100TB with two options`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Storage optimized: 24 vCPU`}</li>\n          <li parentName=\"ol\">{`Compute optimized: 52 vCPU and optional GPU`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Glacier cannot import data directly from snowball, must be done through S3`}</p>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"ii-storage-gateway\"\n    }}>{`II. Storage Gateway`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Bridges gap between on-premise data and cloud data in S3. `}</p>\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Hybrid storage between on-prem and cloud.</Quote>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Exam tips`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`On premise data to cloud → Storage Gateway`}</li>\n          <li parentName=\"ol\">{`File access/NFS → File Gateway`}</li>\n          <li parentName=\"ol\">{`Volume/Block Storage/iSCSI → Volume Gateway`}</li>\n          <li parentName=\"ol\">{`VTL tape solution/Backup with iSCSI → Tape Gateway`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Use cases: disaster recovery, backup & restore`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Configured S3 buckets are accessible via NFS and SMB protocols`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Supports S3 standard, S3 IA and One-zone IA`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Bucket access using IAM roles for each file gateway`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Most recently used data is cached in the file gateway</Quote>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Can be mounted on many servers`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Supports S3 object lock enabling WORM</Quote>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Block storage using `}<inlineCode parentName=\"p\">{`iSCSI`}</inlineCode>{` protocol backed by S3`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Backed by EBS snapshots which can help restore on-premise volumes`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Supports up to 32 volumes`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Cached volumes:</Quote> entire dataset on S3 but frequently accessed cached locally (1GB-32TB)\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Stored volumes:</Quote> entire dataset is on-premise, scheduled backups to S3 (1GB-16TB)\n      </li>\n    </ol>\n    <p><img alt={\"Volume Gateway\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/volumeget.png\").default} /></p>\n    <ol {...{\n      \"start\": 3\n    }}>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Tape Gateway</Quote>\n1. Used for backing up <Quote mdxType=\"Quote\">physical tapes</Quote>\n2. Virtual Tape Library (`VTL`) backed by S3 and Glacier\n3. Back up data using existing tape-based processes (and iSCSI interface)\n4. Works with leading backup vendors\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\"><em parentName=\"p\">{`File Gateway — hardware appliance`}</em></p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Using a file gateway means you need virtualization but now you can get a dedicated hardware for that as well.`}</li>\n          <li parentName=\"ol\">{`You can buy a Dell EMC rack from Amazon`}</li>\n          <li parentName=\"ol\">{`helpful for daily NFS backups`}</li>\n        </ol>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"iii-storage-comparison\"\n    }}>{`III. Storage comparison`}</h2>\n    <ol>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`S3:`}</strong>{` Object storage`}</li>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`Glacier:`}</strong>{` Object archival`}</li>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`EFS:`}</strong>{` NFS for POSIX-compliant systems, shared across EC2s`}</li>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`FSx for Windows:`}</strong>{`NFS for Windows servers`}</li>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`FSx for Lustre:`}</strong>{` HPC linux file system`}</li>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`EBS:`}</strong>{` Network storage for one EC2 at a time`}</li>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`Instance Storage:`}</strong>{`Physical storage for EC2 (high IOPS)`}</li>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`Storage Gateway:`}</strong>{` File, Volume and Tape for on-prem to cloud connection`}</li>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`Snowball/Snowmobile:`}</strong>{` Physically move large amounts of data to cloud`}</li>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`Database:`}</strong>{` For specific workloads, querying and indexing`}</li>\n    </ol>\n    <h2 {...{\n      \"id\": \"iv-s3-transfer-acceleration\"\n    }}>{`IV. S3 Transfer Acceleration`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`Fast and secure way of copying files over large distance between you and S3`}</li>\n      <li parentName=\"ol\">{`Leverages CloudFront's global edge network`}</li>\n      <li parentName=\"ol\">{`Good choice if you are uploading to a centralized bucket from various locations`}</li>\n      <li parentName=\"ol\">{`Each time you use acceleration, S3 will first check if it's actual faster than regular upload`}</li>\n      <li parentName=\"ol\">{`Can be used along with Storage Gateway`}</li>\n    </ol>\n    <h2 {...{\n      \"id\": \"iv-s3-transfer-acceleration-1\"\n    }}>{`IV. S3 Transfer Acceleration`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`Fast and secure way of copying files over large distance between you and S3`}</li>\n      <li parentName=\"ol\">{`Leverages CloudFront's global edge network`}</li>\n      <li parentName=\"ol\">{`Good choice if you are uploading to a centralized bucket from various locations`}</li>\n      <li parentName=\"ol\">{`Each time you use acceleration, S3 will first check if it's actual faster than regular upload`}</li>\n      <li parentName=\"ol\">{`Can be used along with Storage Gateway`}</li>\n    </ol>\n    <p>{`## V. Transfer comparisons`}</p>\n    <p><img alt={\"Transfer comparisons\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/trancomp.png\").default} /></p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}