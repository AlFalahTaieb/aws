4BE,UAAI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAqB,kBAArB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EACE,UAAI,UAAU,CAAC,IAAf,EAAsB,sEAAtB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAsB,sDAAtB,CAFF,CAGE,UAAI,UAAU,CAAC,IAAf,EAAsB,+CAAtB,CAHF,CAIE,UAAI,UAAU,CAAC,IAAf,EAAsB,sCAAtB,CAJF,CAKE,UAAI,UAAU,CAAC,IAAf,EAAsB,oCAAtB,CALF,CAFF,CA5BF,CAsCE,UAAI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAqB,kBAArB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EACE,UAAI,UAAU,CAAC,IAAf,EAAsB,4CAAtB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAsB,8BAAtB,CAFF,CAGE,UAAI,UAAU,CAAC,IAAf,EAAsB,0CAAtB,CAHF,CAIE,UAAI,UAAU,CAAC,IAAf,EAAsB,wDAAtB,CAJF,CAKE,UAAI,UAAU,CAAC,IAAf,EAAsB,mDAAtB,CALF,CAFF,CAtCF,CA/BK,CAgFL,aAAG,WAAK,GAAG,CAAE,cAAV,CAA0B,GAAG,CAAEE,OAAO,CAAC,2GAAD,CAAP,CAAqHC,OAApJ,EAAH,CAhFK,CAiFL,SAAQ,CACN,KAAM,iCADA,CAAR,CAEK,kCAFL,CAjFK,CAoFL,cACE,UAAI,UAAU,CAAC,IAAf,EAAsB,0FAAtB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EACE,IAAC,KAAD,EAAO,OAAO,CAAC,OAAf,yGADF,CAFF,CAKE,UAAI,UAAU,CAAC,IAAf,EAAsB,gBAAtB,CACE,UAAI,UAAU,CAAC,IAAf,EACE,UAAI,UAAU,CAAC,IAAf,EAAsB,wFAAtB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAsB,oEAAtB,CAFF,CADF,CALF,CApFK,CAgGL,aAAG,WAAK,GAAG,CAAE,cAAV,CAA0B,GAAG,CAAED,OAAO,CAAC,4GAAD,CAAP,CAAsHC,OAArJ,EAAH,CAhGK,CAAP,CAkGD,CAED,CACAJ,UAAU,CAACK,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tid: 'notes_sw',\n\ttitle: 'Shield & WAF',\n\tsidebar_label: 'Shield & WAF',\n\tauthor: 'Taieb'\n};\nexport const metadata = {\n  \"unversionedId\": \"notes_sw\",\n  \"id\": \"notes_sw\",\n  \"isDocsHomePage\": false,\n  \"title\": \"Shield & WAF\",\n  \"description\": \"export const Quote = ({ children, color }) => (\",\n  \"source\": \"@site/docs/notes_sw.mdx\",\n  \"slug\": \"/notes_sw\",\n  \"permalink\": \"/FAQ/docs/notes_sw\",\n  \"version\": \"current\",\n  \"sidebar_label\": \"Shield & WAF\",\n  \"sidebar\": \"someSidebar\",\n  \"previous\": {\n    \"title\": \"SSM\",\n    \"permalink\": \"/FAQ/docs/notes_ssm\"\n  },\n  \"next\": {\n    \"title\": \"VPC\",\n    \"permalink\": \"/FAQ/docs/notes_vpc\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: 'I. Shield → DDoS',\n  id: 'i-shield-→-ddos',\n  children: []\n}, {\n  value: 'II. WAF → XSS, CSRF, SQL Injection',\n  id: 'ii-waf-→-xss-csrf-sql-injection',\n  children: []\n}, {\n  value: 'III. Shared Responsibility Model',\n  id: 'iii-shared-responsibility-model',\n  children: []\n}];\nexport const Quote = ({\n  children,\n  color\n}) => <span style={{\n  backgroundColor: \"#5a5a5a\",\n  borderRadius: \"8px\",\n  color: \"#FF9900\",\n  padding: \"0.21rem\"\n}}>\n\t\t{children}\n\t</span>;\n\nconst layoutProps = {\n  rightToc,\nQuote\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n\n    {\n      /* ![Example](../src/media/cexample.png) */\n    }\n    <h2 {...{\n      \"id\": \"i-shield-→-ddos\"\n    }}>{`I. Shield → DDoS`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Standard shield`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Free service activated by default for every AWS customer`}</li>\n          <li parentName=\"ol\">{`Layer 4 DDoS protection`}</li>\n          <li parentName=\"ol\">{`Provides protection from attacks such as SYN/UDP floods, Reflection attacks and other Layer 3/4 attacks.`}</li>\n          <li parentName=\"ol\"><strong parentName=\"li\">{`You CANNOT use Shield to protect against XSS attacks, use WAF for that. Shield is used to protect against DDoS attacks.`}</strong></li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Advanced shield`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Optional DDoS mitigation service ($3k per month per org)`}</li>\n          <li parentName=\"ol\">{`Protection against more sophisticated attacks (layer 3, 4 and 7)`}</li>\n          <li parentName=\"ol\">{`24/7 access to DDoS response team`}</li>\n        </ol>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"ii-waf-→-xss-csrf-sql-injection\"\n    }}>{`II. WAF → XSS, CSRF, SQL Injection`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Protects your web apps against common web exploits (Layer 7 aka HTTP)`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Can only be deployed on ALB, CloudFront and API Gateway</Quote>\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Configure rules that allow, block or monitor web requests based on a condition</Quote>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Provides real-time metrics and captures raw requests that include details about IP addresses, `}<strong parentName=\"p\">{`geo locations`}</strong>{`, URIs, User-Agent and referrers.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Layer 7 DDoS protection`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Charges based on number of web ACLs created`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`You can use WAF to protect sites not hosted on AWS`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Pre-configured rules (AWS WAF Managed Rules) available to protect against common security vulnerabilities like OWASP Top 10`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Best option to quickly and temporarily block malicious IPs when compared to CloudFront ACL, EC2 ACL or EC2 security group.</Quote>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Defining Web ACL`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Rules can include IP address, HTTP headers, HTTP body or URI strings`}</li>\n          <li parentName=\"ol\">{`Protects from common attacks — SQL injection and XSS`}</li>\n          <li parentName=\"ol\">{`Size constraints, geo match (block countries)`}</li>\n          <li parentName=\"ol\">{`Rate-based rules for DDoS protection`}</li>\n          <li parentName=\"ol\">{`Block countries based on geo match`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Firewall Manager`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Manage rules in all accounts of an AWS org`}</li>\n          <li parentName=\"ol\">{`Common set of security rules`}</li>\n          <li parentName=\"ol\">{`WAF rules (ALB, API Gateway, CloudFront)`}</li>\n          <li parentName=\"ol\">{`AWS Shield Advanced (ALB, CLB, Elastic IP, CloudFront)`}</li>\n          <li parentName=\"ol\">{`Security groups for EC2 and ENI resources in VPC `}</li>\n        </ol>\n      </li>\n    </ol>\n    <p><img alt={\"Shield & WAF\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/sw.png\").default} /></p>\n    <h2 {...{\n      \"id\": \"iii-shared-responsibility-model\"\n    }}>{`III. Shared Responsibility Model`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`AWS is responsible for protecting the cloud including all the underlying infrastructure.`}</li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Customer is responsible for managing guest OS patching, firewall and network configuration, IAM etc.</Quote>\n      </li>\n      <li parentName=\"ol\">{`In case of S3:`}\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`AWS is responsible for guaranteeing unlimited storage, encryption, separation of data.`}</li>\n          <li parentName=\"ol\">{`Customer is responsible for bucket config, policy, IAM, encryption`}</li>\n        </ol>\n      </li>\n    </ol>\n    <p><img alt={\"Shield & WAF\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/sw1.png\").default} /></p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}