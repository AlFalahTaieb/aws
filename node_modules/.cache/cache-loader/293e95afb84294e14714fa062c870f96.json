JF,CAKE,UAAI,UAAU,CAAC,IAAf,EAAsB,mDAAtB,CALF,CAME,UAAI,UAAU,CAAC,IAAf,EAAsB,wEAAtB,CANF,CA/GK,CAuHL,aAAG,WAAK,GAAG,CAAE,QAAV,CAAoB,GAAG,CAAED,OAAO,CAAC,4GAAD,CAAP,CAAsHC,OAA/I,EAAH,CAvHK,CAAP,CAyHD,CAED,CACAJ,UAAU,CAACK,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tid: 'notes_ec',\n\ttitle: 'ElastiCache',\n\tsidebar_label: 'ElastiCache',\n\tauthor: 'Taieb'\n};\nexport const metadata = {\n  \"unversionedId\": \"notes_ec\",\n  \"id\": \"notes_ec\",\n  \"isDocsHomePage\": false,\n  \"title\": \"ElastiCache\",\n  \"description\": \"export const Quote = ({ children, color }) => (\",\n  \"source\": \"@site/docs/notes_ec.mdx\",\n  \"slug\": \"/notes_ec\",\n  \"permalink\": \"/FAQ/docs/notes_ec\",\n  \"version\": \"current\",\n  \"sidebar_label\": \"ElastiCache\",\n  \"sidebar\": \"someSidebar\",\n  \"previous\": {\n    \"title\": \"Cognito\",\n    \"permalink\": \"/FAQ/docs/notes_cognito\"\n  },\n  \"next\": {\n    \"title\": \"Route 53\",\n    \"permalink\": \"/FAQ/docs/notes_r53\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: 'I. Introduction',\n  id: 'i-introduction',\n  children: []\n}, {\n  value: 'II. Use cases',\n  id: 'ii-use-cases',\n  children: []\n}, {\n  value: 'III. Redis vs MemcacheD',\n  id: 'iii-redis-vs-memcached',\n  children: []\n}, {\n  value: 'IV. Security',\n  id: 'iv-security',\n  children: []\n}, {\n  value: 'V. Patterns',\n  id: 'v-patterns',\n  children: []\n}, {\n  value: 'VI. ElastiCache for SA',\n  id: 'vi-elasticache-for-sa',\n  children: []\n}];\nexport const Quote = ({\n  children,\n  color\n}) => <span style={{\n  backgroundColor: \"#5a5a5a\",\n  borderRadius: \"8px\",\n  color: \"#FF9900\",\n  padding: \"0.21rem\"\n}}>\n\t\t{children}\n\t</span>;\n\nconst layoutProps = {\n  rightToc,\nQuote\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n\n    {\n      /* ![Example](../src/media/cexample.png) */\n    }\n    <h2 {...{\n      \"id\": \"i-introduction\"\n    }}>{`I. Introduction`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Web service to deploy, operate and scale in-memory cache in the cloud.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Improves perf of web-based apps by reducing reads from disks.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`Supports two caching engines`}</strong></p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\"><strong parentName=\"li\">{`redis (advanced use case)`}</strong></li>\n          <li parentName=\"ol\"><strong parentName=\"li\">{`memcached (basic use case)`}</strong></li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Should be used along side read replicas to improve perf of an app.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Read scaling using read replicas.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Multi-AZ with failover capability.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`AWS takes care of OS, patching, optimizations and such.`}</p>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"ii-use-cases\"\n    }}>{`II. Use cases`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`Lazy loading`}</li>\n    </ol>\n    <p><img alt={\"EC AWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/ec1.png\").default} /></p>\n    <ol {...{\n      \"start\": 2\n    }}>\n      <li parentName=\"ol\">{`Shopping cart user session`}</li>\n    </ol>\n    <p><img alt={\"EC AWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/ec2.png\").default} /></p>\n    <h2 {...{\n      \"id\": \"iii-redis-vs-memcached\"\n    }}>{`III. Redis vs MemcacheD`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`Redis`}</strong></p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\"><strong parentName=\"li\">{`Multi-AZ with auto failover`}</strong></li>\n          <li parentName=\"ol\">{`Read replicas to scale reads and have HA`}</li>\n          <li parentName=\"ol\">{`Data durability using AOF persistence`}</li>\n          <li parentName=\"ol\">{`Backup and restore features`}</li>\n          <li parentName=\"ol\">{`Used more like traditional db`}</li>\n          <li parentName=\"ol\">{`Can be used for message queue as well`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`Memcached`}</strong></p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Multi-node for partitioning (sharding)`}</li>\n          <li parentName=\"ol\">{`Non persistent`}</li>\n          <li parentName=\"ol\">{`No backup`}</li>\n          <li parentName=\"ol\">{`Multi-threaded architecture`}</li>\n          <li parentName=\"ol\">{`Used solely for memcached`}</li>\n        </ol>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"iv-security\"\n    }}>{`IV. Security`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`All caches in ElastiCache`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Support SSL`}</li>\n          <li parentName=\"ol\"><strong parentName=\"li\">{`No IAM auth`}</strong></li>\n          <li parentName=\"ol\">{`IAM policies on ElastiCache are only used for AWS API-level security`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`Redis auth`}</strong></p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\"><strong parentName=\"li\">{`You can set a \"password/token\" when you create a cluster`}</strong></li>\n          <li parentName=\"ol\">{`This is extra level of security for your cache (on top of security groups)`}</li>\n          <li parentName=\"ol\">{`Forces user to enter credentials`}</li>\n          <li parentName=\"ol\">{`Use HTTPS to enable in-transit encryption while the data is being replicated.`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Memcached — supports SASL-based auth`}</p>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"v-patterns\"\n    }}>{`V. Patterns`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`Lazy loading: all the read data is cached (can be stale)`}</li>\n      <li parentName=\"ol\">{`Write through: adds/updates data in the cache when written to a db (no stale data)`}</li>\n      <li parentName=\"ol\">{`Session store: store temporary session data in cache (using TTL features)`}</li>\n    </ol>\n    <h2 {...{\n      \"id\": \"vi-elasticache-for-sa\"\n    }}>{`VI. ElastiCache for SA`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`Operations: same as RDS`}</li>\n      <li parentName=\"ol\">{`Security: AWS responsible for OS security, we are responsible for user auth and encryption`}</li>\n      <li parentName=\"ol\">{`Reliability: Clustering, multi-AZ`}</li>\n      <li parentName=\"ol\">{`Performance: Sub-millisecond performance, in-memory, read-replicas for sharding`}</li>\n      <li parentName=\"ol\">{`Cost: Pay per hour based on EC2 and storage usage`}</li>\n      <li parentName=\"ol\">{`Below is a fully-functional 3-tier architecture with different caches `}</li>\n    </ol>\n    <p><img alt={\"EC AWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/ec3.png\").default} /></p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}