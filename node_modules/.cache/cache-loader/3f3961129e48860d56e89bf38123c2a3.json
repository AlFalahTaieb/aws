IAAC,YAAD,EAAc,IAAI,CAAC,iBAAnB,CAAqC,OAAO,CAAC,cAA7C,gBA3HK,CAAP,CA6HD,CAED,CACAJ,UAAU,CAACK,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tid: 'notes_vpc_n',\n\ttitle: 'Networking',\n\tauthor: 'Taieb'\n};\nexport const metadata = {\n  \"unversionedId\": \"notes_vpc_n\",\n  \"id\": \"notes_vpc_n\",\n  \"isDocsHomePage\": false,\n  \"title\": \"Networking\",\n  \"description\": \"export const StyledButton = withStyles({\",\n  \"source\": \"@site/docs/notes_vpc_n.mdx\",\n  \"slug\": \"/notes_vpc_n\",\n  \"permalink\": \"/FAQ/docs/notes_vpc_n\",\n  \"version\": \"current\"\n};\n  /* @jsx mdx */\nimport Button from \"@material-ui/core/Button\"\nimport { withStyles } from \"@material-ui/core/styles\";\nexport const rightToc = [{\n  value: 'I. Basics',\n  id: 'i-basics',\n  children: []\n}, {\n  value: 'II. CIDR',\n  id: 'ii-cidr',\n  children: []\n}, {\n  value: 'III. Private vs Public',\n  id: 'iii-private-vs-public',\n  children: []\n}];\nexport const StyledButton = withStyles({\n  root: {\n    background: \"linear-gradient(315deg, #fc9842 0%, #fe5f75 74%)\",\n    borderRadius: 7,\n    border: 0,\n    color: \"inherit\",\n    height: 35,\n    padding: \"0 30px\",\n    boxShadow: \"0 3px 5px 2px rgba(255, 105, 135, .3)\"\n  },\n  label: {\n    textTransform: \"capitalize\"\n  }\n})(Button);\n\nconst layoutProps = {\n  rightToc,\nStyledButton\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n\n\n    <h2 {...{\n      \"id\": \"i-basics\"\n    }}>{`I. Basics`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`There are two types of IPs - Public and Private`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Public IP: It is unique and can be easily identified on the internet`}</li>\n          <li parentName=\"ol\">{`Private IP: It is not unique and can only be identified in a private network`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Private IPs can talk on internet when attached to NAT + Internet Gateway.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`An elastic IP is a static IP unless you delete it. AWS provides up to 5 elastic IPs. Avoid it and use DNS name instead.`}</strong></p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`An example IP is written as 192.168.1.0. The Classless Inter-Domain Routing  (CIDR) notation would be 192.168.1.0/24`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`We can then find out the subnet mask from /24. Here, 24 means the first 24 bits are on. `}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`/24 = 11111111.11111111.11111111.00000000 = 255.255.255.0`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Hence, the subnet mask for our network is 255.255.255.0 which means that we can add 254 devices to our network without worrying about gateways etc.`}</p>\n      </li>\n    </ol>\n    <p><img alt={\"VPC AWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/vpc33.png\").default} /></p>\n    <h2 {...{\n      \"id\": \"ii-cidr\"\n    }}>{`II. CIDR`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`They help define an IP address range`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`A CIDR has two components`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Base IP (1.2.3.4)`}</li>\n          <li parentName=\"ol\">{`Subnet mask (/26)`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`The base IP represents an IP contained in a range`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Subnet CIDR cannot overlap`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`All subnets must have an associated route table`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Max CIDR size in AWS is /16 `}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`The subnet mask defines how many bits can change in the IP`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`The subnet mask can take two forms`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`255.255.255.0 (less common)`}</li>\n          <li parentName=\"ol\">{`/24 (more common) `}</li>\n        </ol>\n      </li>\n    </ol>\n    <p><img alt={\"VPC AWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/vpc35.png\").default} /></p>\n    <p><img alt={\"VPC AWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/vpc34.png\").default} /></p>\n    <h2 {...{\n      \"id\": \"iii-private-vs-public\"\n    }}>{`III. Private vs Public`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`IANA mandates certain blocks of IPv4 addresses to private and rest to public.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`For private IPs, `}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Big networks → 10.0.0.0 — 10.255.255.255 (10.0.0.0/8)`}</li>\n          <li parentName=\"ol\">{`Default AWS VPC → 172.16.0.0 — 172.31.255.255 (172.16.0.0/12)`}</li>\n          <li parentName=\"ol\">{`Home networks → 192.168.0.0 — 192.168.255.25 (192.168.0.0/16)`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Everything else is public IP.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`You cannot change the size of an existing CIDR block!`}</strong></p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`When you associate a CIDR block with your VPC, a route table entry is automatically created with the target as `}<strong parentName=\"p\">{`local`}</strong></p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`The CIDR block is ready for use when it is in the `}<inlineCode parentName=\"p\">{`associated`}</inlineCode>{` state.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Elastic IPs are static IPs that you own unless you release them. Avoid using it.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Try using random public IPs and associate them with a DNS name.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`Best way is to use ELB and not use public IP at all.`}</strong></p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`Private subnets do not have a routing table entry to an internet gateway.`}</strong>{` They are not directly accessible from the public internet. Typically use a NAT gateway to support restricted, outbound public internet access. NAT gateways MUST be on public subnet.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`For public subnets, they include a routing table entry to an IGW to support inbound/outbound access to the public internet.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Security Groups are stateful (automatically allow response after a request)`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`NACL act as firewall for subnet. They intake rule based on numbers (100, 200 etc). There's a default NACL for every subnet.`}</p>\n      </li>\n    </ol>\n    <StyledButton href=\"/docs/notes_vpc\" mdxType=\"StyledButton\">Back to VPC</StyledButton>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}