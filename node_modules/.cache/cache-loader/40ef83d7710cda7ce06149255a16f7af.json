AI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAqB,iDAArB,CADF,CADF,CAIE,UAAI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAqB,sBAArB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EACE,UAAI,UAAU,CAAC,IAAf,EAAsB,+BAAtB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAsB,iBAAtB,CAFF,CAFF,CAJF,CAWE,UAAI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAqB,2FAArB,CADF,CAXF,CAcE,UAAI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAqB,yHAArB,CADF,CAdF,CAiBE,UAAI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAqB,mFAArB,CADF,CAjBF,CAoBE,UAAI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAqB,qDAArB,CADF,CApBF,CAuBE,UAAI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAmB,kBAAY,UAAU,CAAC,GAAvB,EAA6B,SAA7B,CAAnB,CAAwE,oDAAxE,CADF,CAvBF,CAzDK,CAoFL,aAAG,WAAK,GAAG,CAAE,eAAV,CAA2B,GAAG,CAAEE,OAAO,CAAC,4GAAD,CAAP,CAAsHC,OAAtJ,EAAH,CApFK,CAAP,CAsFD,CAED,CACAJ,UAAU,CAACK,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tid: 'notes_bs',\n\ttitle: 'BeanStalk',\n\tsidebar_label: 'BeanStalk',\n\tauthor: 'Taieb'\n};\nexport const metadata = {\n  \"unversionedId\": \"notes_bs\",\n  \"id\": \"notes_bs\",\n  \"isDocsHomePage\": false,\n  \"title\": \"BeanStalk\",\n  \"description\": \"export const Quote = ({ children, color }) => (\",\n  \"source\": \"@site/docs/notes_bs.mdx\",\n  \"slug\": \"/notes_bs\",\n  \"permalink\": \"/FAQ/docs/notes_bs\",\n  \"version\": \"current\",\n  \"sidebar_label\": \"BeanStalk\",\n  \"sidebar\": \"someSidebar\",\n  \"previous\": {\n    \"title\": \"CloudFront\",\n    \"permalink\": \"/FAQ/docs/notes_cf\"\n  },\n  \"next\": {\n    \"title\": \"SQS\",\n    \"permalink\": \"/FAQ/docs/notes_sqs\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: 'I. Introduction',\n  id: 'i-introduction',\n  children: []\n}, {\n  value: 'II. Beanstalk Tips',\n  id: 'ii-beanstalk-tips',\n  children: []\n}];\nexport const Quote = ({\n  children,\n  color\n}) => <span style={{\n  backgroundColor: \"#5a5a5a\",\n  borderRadius: \"8px\",\n  color: \"#FF9900\",\n  padding: \"0.21rem\"\n}}>\n\t\t{children}\n\t</span>;\n\nconst layoutProps = {\n  rightToc,\nQuote\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n\n    {\n      /* ![Example](../src/media/cexample.png) */\n    }\n    <h2 {...{\n      \"id\": \"i-introduction\"\n    }}>{`I. Introduction`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`It's a PaaS that quickly deploy and manage \"applications\" (not infra, remember it)`}</strong></p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Supports Go, Java, Node, .NET, Python etc`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Instantiating apps quickly can be done through`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Golden AMI: Install all dependencies/apps beforehand and create an AMI out of it`}</li>\n          <li parentName=\"ol\">{`Bootstrap using user data`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`Elastic Beanstalk is a developer-centric way of deploying a typical app e.g. a 3-tier web-app. It uses all the common components like EC2, EBS, ELB, ASG etc.`}</strong>{` `}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Free of cost and you only pay for underlying services.`}</li>\n          <li parentName=\"ol\">{`Deployment strategy is configurable but performed by Beanstalk.`}</li>\n          <li parentName=\"ol\">{`Developer is only responsible for app`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Three architecture models`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Single instance (good for dev)`}</li>\n          <li parentName=\"ol\">{`LB + ASG (good for production)`}</li>\n          <li parentName=\"ol\">{`ASG only (good for non-web apps)`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Beanstalk has three components`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Application — logical collection of components including env and versions`}</li>\n          <li parentName=\"ol\">{`Application version — labeled iteration of deployable code for a web app`}</li>\n          <li parentName=\"ol\">{`Env — a version that is deployed on to AWS resources`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`You deploy app version to env and can promote envs. Also, roll back to previous version.`}</strong></p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`Elastic Beanstalk stores your application files and optionally, server log files in S3.`}</strong></p>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"ii-beanstalk-tips\"\n    }}>{`II. Beanstalk Tips`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Supports Tomcat, Passenger and Puma containers.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Types of environment`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Load balancing — auto scaling`}</li>\n          <li parentName=\"ol\">{`Single instance`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Beanstalk monitoring console displays your env status and application health at a glance.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Enhanced Health Reporting is a feature that you can enable on your env to allow Beanstalk to gather additional metrics.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`EC2 instances in your Beanstalk env generate logs that can be used for debugging.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Make sure you have the right IAM role to create env`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\"><inlineCode parentName=\"p\">{`eb init`}</inlineCode>{` can be used to initialize a Beanstalk environment`}</p>\n      </li>\n    </ol>\n    <p><img alt={\"BeanStalk AWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/bs1.png\").default} /></p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}