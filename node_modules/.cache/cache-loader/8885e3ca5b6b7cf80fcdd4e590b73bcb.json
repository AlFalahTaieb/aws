ASE,UAAI,UAAU,CAAC,IAAf,EAAsB,4KAAtB,CATF,CAUE,UAAI,UAAU,CAAC,IAAf,EAAsB,sMAAtB,CAA4N,IAAC,KAAD,EAAO,OAAO,CAAC,OAAf,EAAyB,sCAAzB,CAA5N,CAAqS,GAArS,CAVF,CAWE,UAAI,UAAU,CAAC,IAAf,EAAsB,+NAAtB,CAAqP,WAAK,GAAG,CAAE,mBAAV,CAA+B,GAAG,CAAED,OAAO,CAAC,4GAAD,CAAP,CAAsHC,OAA1J,EAArP,CAXF,CAtLK,CA0ML,IAAC,YAAD,EAAc,IAAI,CAAC,iBAAnB,CAAqC,OAAO,CAAC,cAA7C,gBA1MK,CAAP,CA4MD,CAED,CACAJ,UAAU,CAACK,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tid: 'notes_Aurora',\n\ttitle: 'Aurora',\n\tsidebar_label: 'Aurora',\n\tauthor: 'Taieb'\n};\nexport const metadata = {\n  \"unversionedId\": \"notes_Aurora\",\n  \"id\": \"notes_Aurora\",\n  \"isDocsHomePage\": false,\n  \"title\": \"Aurora\",\n  \"description\": \"export const StyledButton = withStyles({\",\n  \"source\": \"@site/docs/notes_aurora.mdx\",\n  \"slug\": \"/notes_Aurora\",\n  \"permalink\": \"/FAQ/docs/notes_Aurora\",\n  \"version\": \"current\",\n  \"sidebar_label\": \"Aurora\"\n};\n  /* @jsx mdx */\nimport Button from \"@material-ui/core/Button\";\nimport { withStyles } from \"@material-ui/core/styles\"; \nexport const rightToc = [{\n  value: 'I. Introduction',\n  id: 'i-introduction',\n  children: []\n}, {\n  value: 'II. Aurora Cluster',\n  id: 'ii-aurora-cluster',\n  children: []\n}, {\n  value: 'III. Aurora Security',\n  id: 'iii-aurora-security',\n  children: []\n}, {\n  value: 'IV. Aurora serverless',\n  id: 'iv-aurora-serverless',\n  children: []\n}, {\n  value: 'V. Aurora Global',\n  id: 'v-aurora-global',\n  children: []\n}, {\n  value: 'VI. Aurora for SA',\n  id: 'vi-aurora-for-sa',\n  children: []\n}, {\n  value: 'VII. Aurora Tips',\n  id: 'vii-aurora-tips',\n  children: []\n}];\nexport const StyledButton = withStyles({\n  root: {\n    background: \"linear-gradient(315deg, #fc9842 0%, #fe5f75 74%)\",\n    borderRadius: 7,\n    border: 0,\n    color: \"inherit\",\n    height: 35,\n    padding: \"0 30px\",\n    boxShadow: \"0 3px 5px 2px rgba(255, 105, 135, .3)\"\n  },\n  label: {\n    textTransform: \"capitalize\"\n  }\n})(Button);\nexport let Quote = ({\n  children,\n  color\n}) => <span style={{\n  backgroundColor: \"#5a5a5a\",\n  borderRadius: \"7px\",\n  color: \"#FF9900\",\n  padding: \"0.3rem\"\n}}>\n\t\t{children}\n</span>;\n\nconst layoutProps = {\n  rightToc,\nStyledButton,\nQuote\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n\n\n\n    <h2 {...{\n      \"id\": \"i-introduction\"\n    }}>{`I. Introduction`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`Aurora serverless provides a simple, cost-effective option for infrequent, intermittent or unpredictable workloads`}</li>\n      <li parentName=\"ol\">{`Cloud-optimized and compatible with MySQL and PostgreSQL`}\n        <ul parentName=\"li\">\n          <li parentName=\"ul\">{`5x better perf than MySQL`}</li>\n          <li parentName=\"ul\">{`3x better perf than PgSQL`}</li>\n          <li parentName=\"ul\">{`+20% than RDS price`}</li>\n        </ul>\n      </li>\n      <li parentName=\"ol\">{`Starts with 10 GB, auto scales in increments of 10 GB (min) up to 64 TB`}</li>\n      <li parentName=\"ol\">{`Compute resources can scale up to 32vCPUs and 244 GB of memory`}</li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Auto backup is on by default</Quote>\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Auto patching is on by default</Quote>\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Fault-tolerant and self-healing</Quote>i.e. data blocks and disks are\nscanned and repaired automatically\n      </li>\n      <li parentName=\"ol\">{`Up to `}<Quote mdxType=\"Quote\">{`15 replicas in Aurora`}</Quote>{`, 5 in MySQL and 1 in PostgreSQL`}</li>\n      <li parentName=\"ol\">{`Can share Aurora snapshots with other AWS accounts`}</li>\n      <li parentName=\"ol\">{`Not open source`}</li>\n    </ol>\n    <h2 {...{\n      \"id\": \"ii-aurora-cluster\"\n    }}>{`II. Aurora Cluster`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`An Aurora DB cluster consists of one or more db instances and a cluster volume that manages the data for those db instances.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`An Aurora cluster volume is a virtual database storage volume that spans multiple AZs, each AZ having a copy of the cluster data.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`DB clusters`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">\n            <Quote mdxType=\"Quote\">Primary db instance</Quote>— Supports R/W, one primary instance per\ncluster\n          </li>\n          <li parentName=\"ol\">\n            <Quote mdxType=\"Quote\">Aurora replica (up to 15)</Quote>\n1. Connects to the same storage volume as the primary db instance \n2. Each Aurora\ndb cluster can have up to 15 Aurora replicas in addition to the primary db\ninstance. \n3. Can offload read work from primary (master) \n4. <Quote mdxType=\"Quote\">\n\tAutomated failover for master in less than 30s\n            </Quote>\n          </li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Amazon Aurora automatically divides your database volume into 10GB segments spread across many disks. Each 10GB chunk of your database volume is replicated six ways, across three Availability Zones.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Two copies of your data is contained in each AZ`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Min 3 AZ, so at least 6 copies of the data`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`4/6 copies needed for write`}</li>\n          <li parentName=\"ol\">{`3/6 copies needed for reads`}</li>\n          <li parentName=\"ol\">{`self-healing with P2P replication`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Aurora endpoints`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">\n            <Quote mdxType=\"Quote\">Cluster endpoint</Quote>— connects to the current primary db\ninstance for a db cluster, <Quote mdxType=\"Quote\">only endpoint that can perform write operations</Quote>\n          </li>\n          <li parentName=\"ol\">\n            <Quote mdxType=\"Quote\">Reader endpoint</Quote>\n1. Connects to one of available Aurora replicas for that DB cluster 2. Each\nAurora db cluster has one reader endpoint 3. The reader endpoint provides load-balancing\nsupport for read-only connections to the db cluster 4. Can't use for writes\n          </li>\n          <li parentName=\"ol\">\n            <Quote mdxType=\"Quote\">Instance endpoint</Quote>— connects to a particular instance in the\ncluster\n          </li>\n        </ol>\n      </li>\n    </ol>\n    <p><img alt={\"Aurora AWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/aurora.png\").default} /></p>\n    <h2 {...{\n      \"id\": \"iii-aurora-security\"\n    }}>{`III. Aurora Security`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`Similar to RDS because uses the same engine`}</li>\n      <li parentName=\"ol\">{`Encryption at rest using KMS`}</li>\n      <li parentName=\"ol\">{`Automated backups, snapshots and replicas are also encrypted`}</li>\n      <li parentName=\"ol\">{`Encryption in-flight using SSL/TLS`}</li>\n      <li parentName=\"ol\">{`Option to authenticate using IAM`}</li>\n      <li parentName=\"ol\">{`Cannot ssh into them`}</li>\n    </ol>\n    <h2 {...{\n      \"id\": \"iv-aurora-serverless\"\n    }}>{`IV. Aurora serverless`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`On-demand, autoscaling configuration for the MySQL-compatible and PostgreSQL-compatible editions of Amazon Aurora. An Aurora Serverless DB cluster automatically starts up, shuts down, and scales capacity up or down based on your application's needs.`}</li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">You can migrate regular Aurora to Aurora serverless</Quote>\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Good for infrequent, intermittent or unpredictable workloads.</Quote>\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">No capacity planning needed.</Quote>\n      </li>\n      <li parentName=\"ol\">{`Does not support fast failover but does support automatic multi-AZ failover i.e. Aurora will automatically recreate the DB instance in a different AZ.`}</li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">You can set min and max Aurora Capacity Unit (ACU)</Quote>\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Pay-per-second, can be more cost effective.</Quote>\n      </li>\n      <li parentName=\"ol\">{`Only available for MySQL and Pgsql`}</li>\n      <li parentName=\"ol\">{`Cannot access publicly, only through a VPC`}</li>\n      <li parentName=\"ol\">{`Does not support`}\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Import/export from S3`}</li>\n          <li parentName=\"ol\">{`Aurora Read Replicas`}</li>\n          <li parentName=\"ol\">{`IAM database auth`}</li>\n          <li parentName=\"ol\">{`Multi-master clusters`}</li>\n          <li parentName=\"ol\">{`Restoring a snapshot from MySQL`}</li>\n        </ol>\n      </li>\n    </ol>\n    <p><img alt={\"Aurora AWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/aurora1.png\").default} /></p>\n    <h2 {...{\n      \"id\": \"v-aurora-global\"\n    }}>{`V. Aurora Global`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Aurora cross-region worldwide read replicas.</Quote>\n1. <Quote mdxType=\"Quote\">Useful for disaster recovery</Quote>\n2. Simple to put in place\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Aurora Global Database (recommended)`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`1 primary region (read/write)`}</li>\n          <li parentName=\"ol\">{`Up to 5 secondary (read-only) regions, replication lag < 1s`}</li>\n          <li parentName=\"ol\">{`Up to 15 read replicas per region`}</li>\n          <li parentName=\"ol\">{`Promoting to another region can be done in <1 minute`}</li>\n          <li parentName=\"ol\">{`You can assign promotion priority for each replica`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">\n\tAurora Global is NOT active-active or multi-master. Use DynamoDB global\n\ttables instead.\n        </Quote>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"vi-aurora-for-sa\"\n    }}>{`VI. Aurora for SA`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`Operations: Less, auto scaling storage`}</li>\n      <li parentName=\"ol\">{`Security: AWS responsible for OS security, we are responsible for setting up KMS, security groups, IAM policies and authorizing users in DB`}</li>\n      <li parentName=\"ol\">{`Reliability: Multi-AZ, HA`}</li>\n      <li parentName=\"ol\">{`Performance: 5x better than MySQL, up to 15 read replicas`}</li>\n      <li parentName=\"ol\">{`Cost: Pay per hour based on EC2 and storage usage. Possibly less than EC2.`}</li>\n    </ol>\n    <h2 {...{\n      \"id\": \"vii-aurora-tips\"\n    }}>{`VII. Aurora Tips`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`Super cost effective throughput (compared to DynamoDB)`}</li>\n      <li parentName=\"ol\">{`Aurora Global = Replication and DR`}</li>\n      <li parentName=\"ol\">{`Aurora Serverless = No hardware provisioning`}</li>\n      <li parentName=\"ol\">{`Recovers almost instantaneously from crash`}</li>\n      <li parentName=\"ol\">{`No free tier`}</li>\n      <li parentName=\"ol\">{`No replication lag`}</li>\n      <li parentName=\"ol\">{`You can setup replication between Aurora and external MySQL db`}</li>\n      <li parentName=\"ol\">{`Cannot encrypt an existing unencrypted instance`}</li>\n      <li parentName=\"ol\">{`Amazon Aurora Parallel Query refers to the ability to push down and distribute the computational load of a single query across thousands of CPUs in Aurora’s storage layer`}</li>\n      <li parentName=\"ol\">{`As well as providing scaling for reads, Aurora Replicas are also targets for multi-AZ. In this case the solutions architect can update the application to read from the Multi-AZ standby instance. (`}<Quote mdxType=\"Quote\">{`For Aurora, Multi AZ ~ Read Replicas`}</Quote>{`)`}</li>\n      <li parentName=\"ol\">{`If using a single Aurora (no multi-AZ, no read replicas), then in case of failure, Aurora will attempt to create a new DB Instance in the same Availability Zone as the original instance and is done on a best-effort basis.`}<img alt={\"Read Replicas AWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/rr2.png\").default} />\n        {\n          /* \n          <Button variant=\"outlined\" color=\"inherit\" href=\"/docs/notes_rds\">\n          Back To RDS\n          </Button> */\n        }\n      </li>\n    </ol>\n    <StyledButton href=\"/docs/notes_rds\" mdxType=\"StyledButton\">Back to RDS</StyledButton>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}