IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tid: 'notes_databases',\n\ttitle: 'Databases',\n\tsidebar_label: 'Databases',\n\tauthor: 'Taieb'\n};\nexport const metadata = {\n  \"unversionedId\": \"notes_databases\",\n  \"id\": \"notes_databases\",\n  \"isDocsHomePage\": false,\n  \"title\": \"Databases\",\n  \"description\": \"export const StyledButton = withStyles({\",\n  \"source\": \"@site/docs/notes_databases.mdx\",\n  \"slug\": \"/notes_databases\",\n  \"permalink\": \"/FAQ/docs/notes_databases\",\n  \"version\": \"current\",\n  \"sidebar_label\": \"Databases\"\n};\n  /* @jsx mdx */\nimport Button from \"@material-ui/core/Button\"\nimport { withStyles } from \"@material-ui/core/styles\";\nexport const rightToc = [];\nexport const StyledButton = withStyles({\n  root: {\n    background: \"linear-gradient(315deg, #fc9842 0%, #fe5f75 74%)\",\n    borderRadius: 7,\n    border: 0,\n    color: \"inherit\",\n    height: 35,\n    padding: \"0 30px\",\n    boxShadow: \"0 3px 5px 2px rgba(255, 105, 135, .3)\"\n  },\n  label: {\n    textTransform: \"capitalize\"\n  }\n})(Button);\n\nconst layoutProps = {\n  rightToc,\nStyledButton\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n\n\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Choosing the right database`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Read-heavy, write-heavy or balanced workload?`}</li>\n          <li parentName=\"ol\">{`Throughput needs? Will it change gradually or fluctuate in bursts?`}</li>\n          <li parentName=\"ol\">{`How much data to store and for how long? Will it grow? Average object size?`}</li>\n          <li parentName=\"ol\">{`How are they accessed?`}</li>\n          <li parentName=\"ol\">{`Data durability? Source of truth?`}</li>\n          <li parentName=\"ol\">{`Latency requirements? Concurrent users?`}</li>\n          <li parentName=\"ol\">{`Data model? Structured vs unstructured?`}</li>\n          <li parentName=\"ol\">{`Strong schema? Flexible schema? Reporting?`}</li>\n          <li parentName=\"ol\">{`License cost? Switch to cloud native?`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`RDBMS`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`SQL and OLTP`}</li>\n          <li parentName=\"ol\">{`RDS, Aurora (great for joins)`}</li>\n          <li parentName=\"ol\">{`Tabular and normalized data`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`NoSQL`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`DynamoDB, ElastiCache, Neptune`}</li>\n          <li parentName=\"ol\">{`No joins`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Object store`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`S3 for big objects`}</li>\n          <li parentName=\"ol\">{`Glacier (for backup)`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Data warehouse`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Redshift (OLAP)`}</li>\n          <li parentName=\"ol\">{`Athena`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Search: ES for text and unstructured data`}</p>\n      </li>\n    </ol>\n    <StyledButton href=\"/docs/notes_rds\" mdxType=\"StyledButton\">Back to RDS</StyledButton>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}