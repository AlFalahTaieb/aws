CA8GL,cACE,UAAI,UAAU,CAAC,IAAf,EAAsB,gDAAtB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAsB,iDAAtB,CAFF,CAGE,UAAI,UAAU,CAAC,IAAf,EAAsB,mBAAtB,CAAyC,kBAAY,UAAU,CAAC,IAAvB,EAA8B,oBAA9B,CAAzC,CAHF,CAIE,UAAI,UAAU,CAAC,IAAf,EACE,IAAC,KAAD,EAAO,OAAO,CAAC,OAAf,yEADF,CAJF,CAOE,UAAI,UAAU,CAAC,IAAf,EAAsB,qGAAtB,CACE,UAAI,UAAU,CAAC,IAAf,EACE,UAAI,UAAU,CAAC,IAAf,EAAsB,0EAAtB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAsB,eAAtB,CAAqC,kBAAY,UAAU,CAAC,IAAvB,EAA8B,YAA9B,CAArC,CAA8F,MAA9F,CAAoG,kBAAY,UAAU,CAAC,IAAvB,EAA8B,oBAA9B,CAApG,CAAqK,uEAArK,CAFF,CAGE,UAAI,UAAU,CAAC,IAAf,EACE,IAAC,KAAD,EAAO,OAAO,CAAC,OAAf,8GADF,CAHF,CAME,UAAI,UAAU,CAAC,IAAf,EACE,IAAC,KAAD,EAAO,OAAO,CAAC,OAAf,4DADF,CANF,CASE,UAAI,UAAU,CAAC,IAAf,EAAsB,2CAAtB,CATF,CADF,CAYE,UAAI,UAAU,CAAC,IAAf,EAZF,CAPF,CA9GK,CAoIL,aAAG,WAAK,GAAG,CAAE,YAAV,CAAwB,GAAG,CAAED,OAAO,CAAC,iHAAD,CAAP,CAA2HC,OAAxJ,EAAH,CApIK,CAqIL,SAAQ,CACN,KAAM,gBADA,CAAR,CAEK,iBAFL,CArIK,CAwIL,cACE,UAAI,UAAU,CAAC,IAAf,EAAoB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,yOAA1B,CAApB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAoB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,6GAA1B,CAApB,CAAsK,sOAAtK,CAFF,CAGE,UAAI,UAAU,CAAC,IAAf,EAAoB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,0DAA1B,CAApB,CAHF,CAIE,UAAI,UAAU,CAAC,IAAf,EAAsB,sEAAtB,CAJF,CAKE,UAAI,UAAU,CAAC,IAAf,EAAsB,yEAAtB,CALF,CAME,UAAI,UAAU,CAAC,IAAf,EAAoB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,4JAA1B,CAApB,CANF,CAOE,UAAI,UAAU,CAAC,IAAf,EAAsB,yHAAtB,CAPF,CAQE,UAAI,UAAU,CAAC,IAAf,EAAoB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,6JAA1B,CAApB,CARF,CASE,UAAI,UAAU,CAAC,IAAf,EAAsB,2HAAtB,CATF,CAUE,UAAI,UAAU,CAAC,IAAf,EAAsB,6IAAtB,CAVF,CAxIK,CAoJL,SAAQ,CACN,KAAM,UADA,CAAR,CAEK,UAFL,CApJK,CAuJL,aAAG,WAAK,GAAG,CAAE,YAAV,CAAwB,GAAG,CAAED,OAAO,CAAC,iHAAD,CAAP,CAA2HC,OAAxJ,EAAH,CAvJK,CAwJL,aAAG,WAAK,GAAG,CAAE,YAAV,CAAwB,GAAG,CAAED,OAAO,CAAC,iHAAD,CAAP,CAA2HC,OAAxJ,EAAH,CAxJK,CAAP,CA0JD,CAED,CACAJ,UAAU,CAACK,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tid: 'notes_cognito',\n\ttitle: 'Cognito',\n\tsidebar_label: 'Cognito',\n\tauthor: 'Taieb'\n};\nexport const metadata = {\n  \"unversionedId\": \"notes_cognito\",\n  \"id\": \"notes_cognito\",\n  \"isDocsHomePage\": false,\n  \"title\": \"Cognito\",\n  \"description\": \"export const Quote = ({ children, color }) => (\",\n  \"source\": \"@site/docs/notes_cognito.mdx\",\n  \"slug\": \"/notes_cognito\",\n  \"permalink\": \"/FAQ/docs/notes_cognito\",\n  \"version\": \"current\",\n  \"sidebar_label\": \"Cognito\",\n  \"sidebar\": \"someSidebar\",\n  \"previous\": {\n    \"title\": \"API Gateway\",\n    \"permalink\": \"/FAQ/docs/notes_apig\"\n  },\n  \"next\": {\n    \"title\": \"ElastiCache\",\n    \"permalink\": \"/FAQ/docs/notes_ec\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: 'I. Introduction',\n  id: 'i-introduction',\n  children: []\n}, {\n  value: 'II. Identity federation',\n  id: 'ii-identity-federation',\n  children: []\n}, {\n  value: 'III. SAML and SSO',\n  id: 'iii-saml-and-sso',\n  children: []\n}, {\n  value: 'V. Cognito Tips',\n  id: 'v-cognito-tips',\n  children: []\n}, {\n  value: 'Examples',\n  id: 'examples',\n  children: []\n}];\nexport const Quote = ({\n  children,\n  color\n}) => <span style={{\n  backgroundColor: \"#5a5a5a\",\n  borderRadius: \"8px\",\n  color: \"#FF9900\",\n  padding: \"0.21rem\"\n}}>\n\t\t{children}\n\t</span>;\n\nconst layoutProps = {\n  rightToc,\nQuote\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n\n    <h2 {...{\n      \"id\": \"i-introduction\"\n    }}>{`I. Introduction`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`We want to give our users an identity so they can interact with our apps.`}</li>\n      <li parentName=\"ol\">{`Cognito helps us by not creating millions of IAM users for our app.`}</li>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`Cognito User Pool`}</strong>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">\n            <Quote mdxType=\"Quote\">A user pool is like a directory of users</Quote>\n          </li>\n          <li parentName=\"ol\">{`Sign-in functionality for app users`}</li>\n          <li parentName=\"ol\">{`Integrate with API Gateway`}</li>\n          <li parentName=\"ol\">{`Create a serverless db for your app users`}\n            <ol parentName=\"li\">\n              <li parentName=\"ol\">{`Simple login (username/email and password)`}</li>\n              <li parentName=\"ol\">{`Support for email verification and MFA`}</li>\n              <li parentName=\"ol\">{`Can enable federated identities (Facebook, Google etc). Sends back a JWT which can then be used for auth by API Gateway`}</li>\n            </ol>\n          </li>\n        </ol>\n      </li>\n    </ol>\n    <p><img alt={\"CognitoAWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/cognito1.png\").default} /></p>\n    <ol {...{\n      \"start\": 4\n    }}>\n      <li parentName=\"ol\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`Cognito Identity Pool (Federated identity)`}</strong></p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">\n            <p parentName=\"li\"><strong parentName=\"p\">{`Federate users into your AWS services`}</strong></p>\n          </li>\n          <li parentName=\"ol\">\n            <p parentName=\"li\">{`Supports anonymous guest users and following IdP:`}</p>\n            <ol parentName=\"li\">\n              <li parentName=\"ol\">{`Cognito user pools`}</li>\n              <li parentName=\"ol\">{`SAML providers`}</li>\n              <li parentName=\"ol\">{`OIDC providers`}</li>\n              <li parentName=\"ol\">{`Social sign-in`}</li>\n              <li parentName=\"ol\">{`Developer authenticated identities `}</li>\n            </ol>\n          </li>\n        </ol>\n      </li>\n    </ol>\n    <p><img alt={\"CognitoAWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/cognito2.png\").default} /></p>\n    <ol {...{\n      \"start\": 3\n    }}>\n      <li parentName=\"ol\">{`Provide AWS credentials to users so they can directly access AWS resources`}</li>\n      <li parentName=\"ol\">{`Integrate with user pools as an identity provider`}</li>\n      <li parentName=\"ol\">{`Support unauthenticated identities by providing a unique identifier and AWS credential`}</li>\n      <li parentName=\"ol\">{`Permission for authed and non-authed users are controlled through IAM roles`}</li>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`No proxy, no API, straight access to AWS resources`}</strong>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Login to federated identity provider or remain anonymous`}</li>\n          <li parentName=\"ol\">{`Get temporary AWS credentials back from federated identity pool`}</li>\n          <li parentName=\"ol\"><strong parentName=\"li\">{`These credentials come with pre-defined IAM policy stating their permissions`}</strong></li>\n          <li parentName=\"ol\">{`Example: Provide temp access to write to S3 using facebook login`}</li>\n        </ol>\n      </li>\n    </ol>\n    <p><img alt={\"CognitoAWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/cognito3.png\").default} /></p>\n    <ol {...{\n      \"start\": 5\n    }}>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Cognito Sync`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Synchronize data from device to Cognito`}</li>\n          <li parentName=\"ol\">{`Maybe deprecated and replaced by AppSync`}</li>\n          <li parentName=\"ol\">{`Store preferences, config, state of app`}</li>\n          <li parentName=\"ol\">{`Cross-device sync across platforms`}</li>\n          <li parentName=\"ol\">{`Offline capability (sync when back online)`}</li>\n          <li parentName=\"ol\">{`Requires federated identity pool`}</li>\n          <li parentName=\"ol\">{`Store data in datasets (up to 1MB)`}</li>\n          <li parentName=\"ol\">{`Up to 20 datasets to synchronize`}</li>\n        </ol>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"ii-identity-federation\"\n    }}>{`II. Identity federation`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Federation lets users outside AWS to assume temporary role to access AWS resources. In other words, you don't need to create IAM users.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Federation can have many flavors`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`SAML 2.0`}</li>\n          <li parentName=\"ol\">{`Custom identity broker`}</li>\n          <li parentName=\"ol\">{`Web identity federation with Cognito`}</li>\n          <li parentName=\"ol\">{`Web identity federation without Cognito`}</li>\n          <li parentName=\"ol\">{`SSO`}</li>\n          <li parentName=\"ol\">{`Non-SAML with Microsoft AD`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Example with any SAML 2.0 compliant IdP`}</p>\n      </li>\n    </ol>\n    <p><img alt={\"CognitoAWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/cognito4.png\").default} /></p>\n    <h2 {...{\n      \"id\": \"iii-saml-and-sso\"\n    }}>{`III. SAML and SSO`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`Needs to set up trust between AWS IAM and SAML`}</li>\n      <li parentName=\"ol\">{`SAML 2.0 enables web-based and cross-domain SSO`}</li>\n      <li parentName=\"ol\">{`Use the STS API: `}<inlineCode parentName=\"li\">{`AssumeRoleWithSAML`}</inlineCode></li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Note: SAML is the old way of doing things, the new way is Amazon SSO</Quote>\n      </li>\n      <li parentName=\"ol\">{`The steps performed by the custom identity broker to sign users into the AWS management console are`}\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Verify that the user is authenticated by your local identity system (AD)`}</li>\n          <li parentName=\"ol\">{`Call AWS STS `}<inlineCode parentName=\"li\">{`AssumeRole`}</inlineCode>{` or `}<inlineCode parentName=\"li\">{`GetFederationToken`}</inlineCode>{` API operations to obtain temporary security credentials for the user`}</li>\n          <li parentName=\"ol\">\n            <Quote mdxType=\"Quote\">Call the AWS federation endpoint and supply the temporary security credentials to request a sign-in token</Quote>\n          </li>\n          <li parentName=\"ol\">\n            <Quote mdxType=\"Quote\">Construct a URL for the console that includes the token</Quote>\n          </li>\n          <li parentName=\"ol\">{`Give the URL to the user (coming from AD)`}</li>\n        </ol>\n        <hr parentName=\"li\"></hr>\n      </li>\n    </ol>\n    <p><img alt={\"CognitoAWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/cognito5.png\").default} /></p>\n    <h2 {...{\n      \"id\": \"v-cognito-tips\"\n    }}>{`V. Cognito Tips`}</h2>\n    <ol>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`ALB supports authentication from OIDC compliant identity providers such as Google, Facebook and Amazon. It is implemented through an authentication action on a listener rule that integrates with Amazon Cognito to create user pools.`}</strong></li>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`Single sign-on using federation allows users to login to the AWS console without assigning IAM credentials.`}</strong>{` The AWS Security Token Service (STS) is a web service that enables you to request temporary, limited-privilege credentials for IAM users or for users that you authenticate (such as federated users from an on-premise directory).`}</li>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`Want to provide SSO to on-prem users â†’ Use STS and SAML.`}</strong></li>\n      <li parentName=\"ol\">{`Cognito Sync is a key/value pair store linked to an Amazon identity.`}</li>\n      <li parentName=\"ol\">{`You can use Cognito streams to push sync store data to Kinesis streams.`}</li>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`When Cognito detects unusual sign-in activity (new location, device etc), it assigns it a risk score to that activity which you can use to enable MFA etc.`}</strong></li>\n      <li parentName=\"ol\">{`When Cognito detects users have entered credentials that have been compromised elsewhere, it prompts a password change.`}</li>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`You can create lambda function and then trigger it during user pool operations like sign-in, sign-up e.g. to present auth challenges to users during log-in`}</strong></li>\n      <li parentName=\"ol\">{`Cognito calls lambda sync, and lambda must respond within 5s. Cognito would then retry the call for a maximum of 3 times.`}</li>\n      <li parentName=\"ol\">{`You pay based on MAUs. The free tier includes 50,000 MAUs signing through user pools or social logins and 50 MAUs for SAML-based providers.`}</li>\n    </ol>\n    <h2 {...{\n      \"id\": \"examples\"\n    }}>{`Examples`}</h2>\n    <p><img alt={\"CognitoAWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/cognito6.png\").default} /></p>\n    <p><img alt={\"CognitoAWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/cognito7.png\").default} /></p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}