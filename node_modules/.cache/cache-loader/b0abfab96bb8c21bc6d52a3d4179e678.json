UAAU,CAAC,IAAf,EAAsB,uCAAtB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAsB,wCAAtB,CAFF,CAGE,UAAI,UAAU,CAAC,IAAf,EAAsB,yBAAtB,CAHF,CAIE,UAAI,UAAU,CAAC,IAAf,EAAsB,iEAAtB,CAJF,CAFF,CAdF,CAxGK,CAgIL,SAAQ,CACN,KAAM,qBADA,CAAR,CAEK,sBAFL,CAhIK,CAmIL,cACE,UAAI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAqB,kEAArB,CADF,CADF,CAIE,UAAI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAqB,mBAArB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EACE,UAAI,UAAU,CAAC,IAAf,EAAsB,wCAAtB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAsB,4BAAtB,CAFF,CAGE,UAAI,UAAU,CAAC,IAAf,EAAsB,yCAAtB,CAHF,CAIE,UAAI,UAAU,CAAC,IAAf,EAAsB,8CAAtB,CAJF,CAKE,UAAI,UAAU,CAAC,IAAf,EAAsB,wEAAtB,CALF,CAFF,CAJF,CAcE,UAAI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAqB,cAArB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EACE,UAAI,UAAU,CAAC,IAAf,EAAsB,2EAAtB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAsB,wDAAtB,CAFF,CAGE,UAAI,UAAU,CAAC,IAAf,EAAsB,mCAAtB,CAHF,CAIE,UAAI,UAAU,CAAC,IAAf,EAAsB,sCAAtB,CAJF,CAFF,CAdF,CAnIK,CAAP,CA4JD,CAED,CACAD,UAAU,CAACG,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tid: 'notes_fp',\n\ttitle: 'Five Pillars',\n\tauthor: 'Taieb'\n};\nexport const metadata = {\n  \"unversionedId\": \"notes_fp\",\n  \"id\": \"notes_fp\",\n  \"isDocsHomePage\": false,\n  \"title\": \"Five Pillars\",\n  \"description\": \"export const Quote = ({ children, color }) => (\",\n  \"source\": \"@site/docs/notes_fp.mdx\",\n  \"slug\": \"/notes_fp\",\n  \"permalink\": \"/FAQ/docs/notes_fp\",\n  \"version\": \"current\",\n  \"sidebar\": \"someSidebar\",\n  \"previous\": {\n    \"title\": \"HPC\",\n    \"permalink\": \"/FAQ/docs/notes_hpc\"\n  },\n  \"next\": {\n    \"title\": \"Notes for AWS Certified Solutions Architect Associate\",\n    \"permalink\": \"/FAQ/docs/notes\"\n  }\n};\n  /* @jsx mdx */\nimport Tabs from '@theme/Tabs';\nimport TabItem from '@theme/TabItem';\nexport const rightToc = [{\n  value: 'General guiding principles',\n  id: 'general-guiding-principles',\n  children: []\n}, {\n  value: 'I. Operational Excellence',\n  id: 'i-operational-excellence',\n  children: []\n}, {\n  value: 'II. Security',\n  id: 'ii-security',\n  children: []\n}, {\n  value: 'III. Reliability',\n  id: 'iii-reliability',\n  children: []\n}, {\n  value: 'IV. Performance Efficiency',\n  id: 'iv-performance-efficiency',\n  children: []\n}, {\n  value: 'V. Cost optimization',\n  id: 'v-cost-optimization',\n  children: []\n}];\nexport const Quote = ({\n  children,\n  color\n}) => <span style={{\n  backgroundColor: \"#5a5a5a\",\n  borderRadius: \"8px\",\n  color: \"#FF9900\",\n  padding: \"0.21rem\"\n}}>\n\t\t{children}\n\t</span>;\n\nconst layoutProps = {\n  rightToc,\nQuote\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n\n\n    {\n      /* ![exp AWS](../src/media/exp.png) */\n    }\n    <h2 {...{\n      \"id\": \"general-guiding-principles\"\n    }}>{`General guiding principles`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`Stop guessing your capacity needs`}</li>\n      <li parentName=\"ol\">{`Test systems at production scale`}</li>\n      <li parentName=\"ol\">{`Automate to make architectural experimentation easier`}</li>\n      <li parentName=\"ol\">{`Allow for evolutionary architectures (changing requirements)`}</li>\n      <li parentName=\"ol\">{`Drive architectures using data`}</li>\n      <li parentName=\"ol\">{`Improve through game days e.g. simulate apps for flash sale days`}</li>\n    </ol>\n    <h2 {...{\n      \"id\": \"i-operational-excellence\"\n    }}>{`I. Operational Excellence`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Includes the ability to run and monitor systems and deliver business value and to continually improve supporting processes and procedures. `}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Design principles`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Perform operations as a code (IaaS)`}</li>\n          <li parentName=\"ol\">{`Annotate docs (Auto docs after every build)`}</li>\n          <li parentName=\"ol\">{`Make frequent, small, reversible changes (push small and frequently)`}</li>\n          <li parentName=\"ol\">{`Refine operations procedures frequently`}</li>\n          <li parentName=\"ol\">{`Anticipate failure`}</li>\n          <li parentName=\"ol\">{`Learn from all operational failures`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`AWS operational excellence`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Prepare — CloudFormation, Config`}</li>\n          <li parentName=\"ol\">{`Operate — CloudFormation, Config, CloudTrail, CoudWatch, X-Ray`}</li>\n          <li parentName=\"ol\">{`Evolve — CloudFormation, CodeBuild, CodePipeline, CodeDeploy`}</li>\n        </ol>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"ii-security\"\n    }}>{`II. Security`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Includes the ability to protect information, systems and assets while delivering business values through risk assessment and mitigation strategies. `}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Design principles`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Implement a strong identity foundation`}</li>\n          <li parentName=\"ol\">{`Enable traceability`}</li>\n          <li parentName=\"ol\">{`Apply security at all layers (edge, subnet, ALB, VPC)`}</li>\n          <li parentName=\"ol\">{`Automate security best practices`}</li>\n          <li parentName=\"ol\">{`Protect data in transit and at rest`}</li>\n          <li parentName=\"ol\">{`Keep people away from data`}</li>\n          <li parentName=\"ol\">{`Prepare for security events`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`AWS services`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`IAM — IAM, STS, Orgs`}</li>\n          <li parentName=\"ol\">{`Detective controls — Config, CloudTrail, CloudWatch`}</li>\n          <li parentName=\"ol\">{`Infra — CloudFront, VPC, WAF, Shield, Inspector`}</li>\n          <li parentName=\"ol\">{`Data protection — KMS, S3, ELB`}</li>\n          <li parentName=\"ol\">{`Incident response — IAM, CloudFormation`}</li>\n        </ol>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"iii-reliability\"\n    }}>{`III. Reliability`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Ability to recover from infra or service disruptions, dynamically acquire computing resources to meet demand, and mitigate disruptions, transient network issues.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Design principles`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Test recovery procedure (simulate failure scenarios)`}</li>\n          <li parentName=\"ol\">{`Automatically recover from failure`}</li>\n          <li parentName=\"ol\">{`Scale horizontally to increase aggregate system availability`}</li>\n          <li parentName=\"ol\">{`Stop guessing capacity (never under-provision)`}</li>\n          <li parentName=\"ol\">{`Manage change in automation`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`AWS`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Foundations — IAM, VPC, Service Limits, Trusted Advisor`}</li>\n          <li parentName=\"ol\">{`Change Management — Auto Scaling, CloudWatch, CLoudTrail, Config`}</li>\n          <li parentName=\"ol\">{`Failure Management — Backups, CloudFormation, S3, Glacier, Route 53`}</li>\n        </ol>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"iv-performance-efficiency\"\n    }}>{`IV. Performance Efficiency`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Includes the ability to use computing resources efficiently to meet system requirements and to maintain that efficiency as demand changes and technology evolve.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Design principles`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Democratize advanced technologies`}</li>\n          <li parentName=\"ol\">{`Go global in minutes`}</li>\n          <li parentName=\"ol\">{`Use serverless as much as you can`}</li>\n          <li parentName=\"ol\">{`Experiment more often`}</li>\n          <li parentName=\"ol\">{`Mechanical sympathy (be on top of AWS services)`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`AWS services`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Selection (ASG, Lambda, EBS, S3, RDS)`}</li>\n          <li parentName=\"ol\">{`Review (CloudFormation, AWS news blog)`}</li>\n          <li parentName=\"ol\">{`Monitoring (CloudWatch)`}</li>\n          <li parentName=\"ol\">{`Tradeoffs (RDS vs Aurora, S3 vs Glacier, Aurora vs ElastiCache)`}</li>\n        </ol>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"v-cost-optimization\"\n    }}>{`V. Cost optimization`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Run systems to deliver business value at the lowest price point.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Design principles`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Adopt a consumption mode (pay per use)`}</li>\n          <li parentName=\"ol\">{`Measure overall efficiency`}</li>\n          <li parentName=\"ol\">{`Stop spending on data center operations`}</li>\n          <li parentName=\"ol\">{`Analyze and attribute expenses e.g. use tags`}</li>\n          <li parentName=\"ol\">{`Use managed and application level services to reduce cost of ownership`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`AWS services`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Expenditure awareness — AWS budgets, Cost and usage report, Cost explorer`}</li>\n          <li parentName=\"ol\">{`Cost-effective resources — Spot vs Reserved vs Glacier`}</li>\n          <li parentName=\"ol\">{`Matching supply and demand —  ASG`}</li>\n          <li parentName=\"ol\">{`Optimize over time — Trusted Advisor`}</li>\n        </ol>\n      </li>\n    </ol>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}