A0HL,SAAQ,CACN,KAAM,cADA,CAAR,CAEK,eAFL,CA1HK,CA6HL,cACE,UAAI,UAAU,CAAC,IAAf,EAAsB,6DAAtB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAsB,8BAAtB,CAFF,CAGE,UAAI,UAAU,CAAC,IAAf,EAAsB,iDAAtB,CAHF,CAIE,UAAI,UAAU,CAAC,IAAf,EAAsB,6BAAtB,CAAmD,kBAAY,UAAU,CAAC,IAAvB,EAA8B,OAA9B,CAAnD,CAAuG,IAAvG,CAJF,CAKE,UAAI,UAAU,CAAC,IAAf,EAAsB,cAAtB,CAAoC,kBAAY,UAAU,CAAC,IAAvB,EAA8B,SAA9B,CAApC,CAA0F,iFAA1F,CALF,CAME,UAAI,UAAU,CAAC,IAAf,EAAsB,2DAAtB,CANF,CA7HK,CAqIL,SAAQ,CACN,KAAM,wBADA,CAAR,CAEK,6BAFL,CArIK,CAwIL,cACE,UAAI,UAAU,CAAC,IAAf,EAAsB,yLAAtB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAsB,qCAAtB,CAFF,CAGE,UAAI,UAAU,CAAC,IAAf,EAAsB,mCAAtB,CAHF,CAIE,UAAI,UAAU,CAAC,IAAf,EAAsB,0DAAtB,CAJF,CAKE,UAAI,UAAU,CAAC,IAAf,EAAsB,mCAAtB,CALF,CAxIK,CA+IL,SAAQ,CACN,KAAM,YADA,CAAR,CAEK,aAFL,CA/IK,CAkJL,cACE,UAAI,UAAU,CAAC,IAAf,EAAsB,4JAAtB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAsB,yJAAtB,CAA+K,cAAQ,UAAU,CAAC,IAAnB,EAA0B,4NAA1B,CAA/K,CAFF,CAGE,UAAI,UAAU,CAAC,IAAf,EACE,IAAC,KAAD,EAAO,OAAO,CAAC,OAAf,iGADF,qKAHF,CAME,UAAI,UAAU,CAAC,IAAf,EAAsB,uNAAtB,CAA6O,kBAAY,UAAU,CAAC,IAAvB,EAA8B,wBAA9B,CAA7O,CAAkT,sBAAlT,CANF,CAOE,UAAI,UAAU,CAAC,IAAf,EAAsB,uCAAtB,CAPF,CAQE,UAAI,UAAU,CAAC,IAAf,EAAsB,oCAAtB,CARF,CASE,UAAI,UAAU,CAAC,IAAf,EAAsB,uEAAtB,CAA6F,kBAAY,UAAU,CAAC,IAAvB,EAA8B,YAA9B,CAA7F,CAAsJ,UAAtJ,CATF,CAUE,UAAI,UAAU,CAAC,IAAf,EAAoB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,wDAA1B,CAApB,CAVF,CAWE,UAAI,UAAU,CAAC,IAAf,EAAoB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,oEAA1B,CAApB,CAXF,CAYE,UAAI,UAAU,CAAC,IAAf,EAAsB,yGAAtB,CAZF,CAaE,UAAI,UAAU,CAAC,IAAf,EAAoB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,wCAA1B,CAApB,CAbF,CAcE,UAAI,UAAU,CAAC,IAAf,EAAsB,mVAAtB,CAdF,CAlJK,CAAP,CAmKD,CAED,CACAJ,UAAU,CAACK,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tid: 'notes_sqs',\n\ttitle: 'SQS',\n\tsidebar_label: 'SQS',\n\tauthor: 'Taieb'\n};\nexport const metadata = {\n  \"unversionedId\": \"notes_sqs\",\n  \"id\": \"notes_sqs\",\n  \"isDocsHomePage\": false,\n  \"title\": \"SQS\",\n  \"description\": \"export const Quote = ({ children, color }) => (\",\n  \"source\": \"@site/docs/notes_sqs.mdx\",\n  \"slug\": \"/notes_sqs\",\n  \"permalink\": \"/FAQ/docs/notes_sqs\",\n  \"version\": \"current\",\n  \"sidebar_label\": \"SQS\",\n  \"sidebar\": \"someSidebar\",\n  \"previous\": {\n    \"title\": \"BeanStalk\",\n    \"permalink\": \"/FAQ/docs/notes_bs\"\n  },\n  \"next\": {\n    \"title\": \"SNS\",\n    \"permalink\": \"/FAQ/docs/notes_sns\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: 'I. Introduction',\n  id: 'i-introduction',\n  children: []\n}, {\n  value: 'II. Message Visibility Timeout',\n  id: 'ii-message-visibility-timeout',\n  children: []\n}, {\n  value: 'III. SQS FIFO',\n  id: 'iii-sqs-fifo',\n  children: []\n}, {\n  value: 'IV. Amazon MQ (&quot;Migration&quot;)',\n  id: 'iv-amazon-mq-migration',\n  children: []\n}, {\n  value: 'V. SQS Tips',\n  id: 'v-sqs-tips',\n  children: []\n}];\nexport const Quote = ({\n  children,\n  color\n}) => <span style={{\n  backgroundColor: \"#5a5a5a\",\n  borderRadius: \"8px\",\n  color: \"#FF9900\",\n  padding: \"0.21rem\"\n}}>\n\t\t{children}\n\t</span>;\n\nconst layoutProps = {\n  rightToc,\nQuote\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n\n    {\n      /* ![Example](../src/media/cexample.png) */\n    }\n    <h2 {...{\n      \"id\": \"i-introduction\"\n    }}>{`I. Introduction`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Oldest AWS service, used to decouple applications.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`Unlimited throughput and number of messages in the queue.`}</strong></p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`Scales automatically, millions and millions of messages.`}</strong></p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Default retention of messages is 4 days (max 14 days)`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Low latency (<10ms on publish and receive)`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Limitation of 256KB per message sent including XML, JSON and unformatted text.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Can have duplicate messages (at least once delivery).`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Best effort message ordering`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Producers`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`Send message using SDK`}</li>\n          <li parentName=\"ol\">{`Gets persisted in SQS`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Consumers`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\"><strong parentName=\"li\">{`Apps running on EC2, Lambda etc`}</strong></li>\n          <li parentName=\"ol\">{`Polls SQS for messages`}</li>\n          <li parentName=\"ol\">{`Receive up to 10 messages at a time`}</li>\n          <li parentName=\"ol\">{`Process the messages (write to RDS)`}</li>\n          <li parentName=\"ol\">{`Delete the message`}</li>\n        </ol>\n      </li>\n    </ol>\n    <p><img alt={\"SQS AWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/sqs.png\").default} /></p>\n    <ol {...{\n      \"start\": 11\n    }}>\n      <li parentName=\"ol\">{`SQS plays nicely with ASG`}\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`ASG can add/remove EC2 based on queue length`}</li>\n          <li parentName=\"ol\">{`The info is provided by a CloudWatch metric`}</li>\n        </ol>\n      </li>\n    </ol>\n    <p><img alt={\"SQS AWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/sqs1.png\").default} /></p>\n    <ol {...{\n      \"start\": 12\n    }}>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Security`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">{`In-flight encryption with HTTPS API`}</li>\n          <li parentName=\"ol\">{`At-rest using KMS`}</li>\n          <li parentName=\"ol\">{`Client-side available as well`}</li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`IAM policies to regulate access to SQS API.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`SQS Access Policies (similar to S3 bucket policies) that are useful for cross-account SQS queues.`}</p>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"ii-message-visibility-timeout\"\n    }}>{`II. Message Visibility Timeout`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`Once a message is polled by a consumer, it becomes \"invisible\" to other consumers.`}</li>\n      <li parentName=\"ol\">{`By default, the \"message visibility timeout\" is 30s which means the message has 30s to get processed (and deleted).`}</li>\n      <li parentName=\"ol\">{`If the message is not processed within 30s, it will be \"visible\" again.`}</li>\n    </ol>\n    <p><img alt={\"SQS AWS\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../src/media/sqs2.png\").default} /></p>\n    <ol {...{\n      \"start\": 4\n    }}>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`If the consumer has seen the message and knows it will take more than 30s to process, it can change the timeout using `}<inlineCode parentName=\"p\">{`ChangeMessageVisibility`}</inlineCode>{` API. This new timeout should be reasonable (not too long, not too short).`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`If the consumer fails to process the message within 30s (can be configured), it will be put back in the queue. If the failure happens again, the message will be keep coming back to the queue. After the `}<inlineCode parentName=\"p\">{`MaximumReceives`}</inlineCode>{` threshold, the message goes to a Dead Letter Queue (DLQ).`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Make sure to process messages in DLQ before they expire.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Good to set a retention period of 14 days.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Delay Queue`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Delay a message so the users don't see it immediately (up to 15 minutes)`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Default is 0s (message is available right away)`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Can set a default at a queue level`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Can override the default using the `}<inlineCode parentName=\"p\">{`DelaySeconds`}</inlineCode>{` parameter`}</p>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"iii-sqs-fifo\"\n    }}>{`III. SQS FIFO`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`Limited throughput 300msgs/s and 3000msgs/s (with batching)`}</li>\n      <li parentName=\"ol\">{`Exactly once send capability`}</li>\n      <li parentName=\"ol\">{`Messages are processed in order by the consumer`}</li>\n      <li parentName=\"ol\">{`Queue name has to end with `}<inlineCode parentName=\"li\">{`.fifo`}</inlineCode>{`. `}</li>\n      <li parentName=\"ol\">{`You can use `}<inlineCode parentName=\"li\">{`GroupID`}</inlineCode>{` (similar to partition key in Kinesis) to send messages to different consumers.`}</li>\n      <li parentName=\"ol\">{`SQS FIFO CANNOT sustain thousands of messages per second.`}</li>\n    </ol>\n    <h2 {...{\n      \"id\": \"iv-amazon-mq-migration\"\n    }}>{`IV. Amazon MQ (\"Migration\")`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`SQS, SNS and Kinesis are cloud-native. If you are already using MQ protocol locally, you would not want to rewrite everything to fit into SQS. This is where Amazon MQ comes into play.`}</li>\n      <li parentName=\"ol\">{`Amazon MQ = Managed Apache ActiveMQ`}</li>\n      <li parentName=\"ol\">{`Doesn't scale as much as SQS/SNS `}</li>\n      <li parentName=\"ol\">{`Runs on a dedicated machine, can run in HA with failover`}</li>\n      <li parentName=\"ol\">{`Has both queue and topic features`}</li>\n    </ol>\n    <h2 {...{\n      \"id\": \"v-sqs-tips\"\n    }}>{`V. SQS Tips`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`Poll the queue after a delayed duration (for 30s over keep-alive connection) to avoid polling too frequently. This is because you are charged per request.`}</li>\n      <li parentName=\"ol\">{`Always remember that the messages in the SQS queue will continue to exist even after the EC2 instance has processed it, until you delete that message. `}<strong parentName=\"li\">{`You have to ensure that you delete the message after processing to prevent the message from being received and processed again once the visibility timeout expires. SQS does not automatically delete messages on its own.`}</strong></li>\n      <li parentName=\"ol\">\n        <Quote mdxType=\"Quote\">Long polling helps reduce the cost of using SQS by eliminating the number of empty responses</Quote> (when there are no messages available for a `ReceiveMessage` request) and false empty responses (when messages are available but aren't included in a response).\n      </li>\n      <li parentName=\"ol\">{`In almost all cases, Amazon SQS long polling is preferable to short polling. Long-polling requests let your queue consumers receive messages as soon as they arrive in your queue while reducing the number of empty `}<inlineCode parentName=\"li\">{`ReceiveMessageResponse`}</inlineCode>{` instances returned.`}</li>\n      <li parentName=\"ol\">{`Monitor SQS queues using CloudWatch. `}</li>\n      <li parentName=\"ol\">{`Log SQS API calls with CloudTrail.`}</li>\n      <li parentName=\"ol\">{`You can delete all messages in an Amazon SQS message queue using the `}<inlineCode parentName=\"li\">{`PurgeQueue`}</inlineCode>{` action.`}</li>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`You CANNOT convert an existing standard queue to FIFO.`}</strong></li>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`When setting up DLQ for FIFO, the DLQ itself must be FIFO as well.`}</strong></li>\n      <li parentName=\"ol\">{`Queues can be encrypted using SSE (using KMS). SSE encrypts the body of a message but not the metadata.`}</li>\n      <li parentName=\"ol\"><strong parentName=\"li\">{`Messages are NOT encrypted by default.`}</strong></li>\n      <li parentName=\"ol\">{`A single Amazon SQS message queue can contain an unlimited number of messages. However, there is a 120,000 limit for the number of inflight messages for a standard queue and 20,000 for a FIFO queue. Messages are inflight after they have been received from the queue by a consuming component, but have not yet been deleted from the queue.`}</li>\n    </ol>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}